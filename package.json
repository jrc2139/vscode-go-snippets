{
  "name": "go-snippets",
  "displayName": "Go Snippets",
  "description": "Code snippets for Go",
  "version": "1.0.0",
  "publisher": "jrc2319",
  "license": "MIT",
  "scripts": {
    "format:sort-json": "sort-json snippets/snippets.json",
    "format:prettier": "prettier --write *.md snippets/*",
    "format": "npm run format:sort-json && npm run format:prettier"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrc2139/vscode-go-snippets"
  },
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": ["Snippets"],
  "contributes": {
    "snippets": [
      {
        "language": "go",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "sort-json": "^2.0.0",
    "vsce": "^1.57.0"
  }
}
